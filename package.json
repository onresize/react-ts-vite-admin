{
	"name": "react-ts-vite-admin",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"preinstall": "yarn config set ignore-engines false && npx npm-only-allow@latest --PM yarn",
		"dev": "vite",
		"devonLine": "npx localtunnel --port 3301",
		"build:lint": "npm run lint-fix && vite build --mode production",
		"build:tsc": "tsc && vite build --mode production && npm run copyfile",
		"copyfile": "node ./script/build-copy.mjs",
		"lint-fix": "eslint --fix --ext .js,.ts,.tsx ./src",
		"lint-prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
		"lint-staged": "lint-staged",
		"tsc": "tsc --noEmit",
		"preview": "vite preview",
		"format-code": "bash .husky/format-code.sh",
		"check-conflict": "bash .husky/check-conflict.sh",
		"check-keyword": "bash .husky/check-keyword.sh",
		"prepare": "husky install",
		"commit": "git add -A && git-cz && git pull && git push",
		"release": "standard-version -- --release-as major"
	},
	"dependencies": {
		"@ant-design/happy-work-theme": "^1.0.0",
		"ahooks": "^3.7.10",
		"antd": "^5.15.3",
		"axios": "^1.6.8",
		"chalk": "^5.3.0",
		"classnames": "^2.5.1",
		"dayjs": "^1.11.10",
		"echarts": "^5.5.0",
		"js-cookie": "^3.0.5",
		"js-md5": "^0.8.3",
		"lodash-es": "^4.17.21",
		"mobx": "^6.12.0",
		"mobx-persist-store": "^1.1.4",
		"mobx-react-lite": "^4.0.6",
		"nprogress": "^0.2.0",
		"qs": "^6.12.0",
		"react": "^18.2.0",
		"react-activation": "^0.12.4",
		"react-dom": "^18.2.0",
		"react-intl": "^6.6.3",
		"react-transition-group": "^4.4.5",
		"screenfull": "^6.0.2",
		"styled-components": "^6.1.8"
	},
	"devDependencies": {
		"@codesandbox/sandpack-react": "^2.13.7",
		"@codesandbox/sandpack-themes": "^2.0.21",
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@types/node": "^20.11.28",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"autoprefixer": "^10.4.19",
		"commitizen": "^4.3.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.9.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"fontmin": "^1.0.1",
		"husky": "^9.0.11",
		"less": "^4.2.0",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"react-router-dom": "^6.22.3",
		"rollup-plugin-visualizer": "^5.12.0",
		"standard-version": "^9.5.0",
		"typescript": "5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-imagemin": "^0.6.1",
		"yorkie": "^2.0.0"
	},
	"gitHooks": {
		"pre-commit": "yorkie pre-commit"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"resolutions": {
		"bin-wrapper": "npm:bin-wrapper-china"
	},
	"engines": {
		"node": "v18.x"
	}
}
